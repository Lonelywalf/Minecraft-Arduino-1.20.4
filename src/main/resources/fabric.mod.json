{
	"schemaVersion": 1,
	"id": "arduinocraft",
	"version": "${version}",
	"name": "Arduino Craft",
	"description": "A mod that can control the pins on an arduino via a block",
	"authors": [
		"Lonelywalf"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "CC0-1.0",
	"icon": "assets/arduinocraft/icon.png",
	"client": "*",
	"entrypoints": {
		"main": [
			"net.jamicah.arduinocraft.Arduinocraft"
		],
		"client": [
			"net.jamicah.arduinocraft.ArduinocraftClient"
		],
		"fabric-datagen": [
			"net.jamicah.arduinocraft.ArduinocraftDataGenerator"
		]
	},
	"mixins": [
		"arduinocraft.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.15.1",
		"minecraft": "~1.20.4",
		"java": ">=17",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}